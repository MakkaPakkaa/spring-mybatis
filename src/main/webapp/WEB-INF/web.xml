<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>AddUser.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <display-name>AddServlet</display-name>
    <servlet-name>AddServlet</servlet-name>
    <servlet-class>com.hry.servlet.AddServlet</servlet-class>
  </servlet>
  <servlet>
  	<display-name>DelServlet</display-name>
  	<servlet-name>DelServlet</servlet-name>
  	<servlet-class>com.hry.servlet.DelServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddServlet</servlet-name>
    <url-pattern>/AddServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DelServlet</servlet-name>
  	<url-pattern>/DelServlet</url-pattern>
  </servlet-mapping>
</web-app>
